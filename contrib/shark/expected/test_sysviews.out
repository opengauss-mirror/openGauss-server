-- prepare data
CREATE TABLE employees (
    name VARCHAR(50) NOT NULL,
    age INT,
    salary DECIMAL(10, 2),
    -- CHECK  >= 18
    CONSTRAINT chk_age CHECK (age >= 18)
);
CREATE VIEW high_salary_employees AS
SELECT name, age, salary
FROM employees
WHERE age >= 18 AND salary > 5000;
INSERT INTO employees (name, age, salary) VALUES ('', 25, 8000.00); 
INSERT INTO employees (name, age, salary) VALUES ('', 26, 4000.00); 
