select * from unnest(get_tab_ptf_autom(2));
select * from table(get_tab_ptf_autom(2));
select unnest(get_tab_ptf_autom(2));
SELECT * from plpgsql_dump_pkg.table_of_rec(6);
select count(*) from stockpivot_pl();
