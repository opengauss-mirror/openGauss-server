update fb_tbl_normal set id = 10 where id = 1;
insert into fb_tbl_normal values(100, 'sfgsafjhaf');
select pg_sleep(4);
insert into temp select 2, to_char(now(), 'yyyy-mm-dd hh24:mi:ss');
