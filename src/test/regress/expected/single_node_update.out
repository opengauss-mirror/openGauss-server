CREATE FUNCTION update_timestamp()
RETURNS trigger
LANGUAGE plpgsql
AUTHID DEFINER NOT FENCED NOT SHIPPABLE
AS $function$
BEGIN
NEW.updateTime = now();
RETURN NEW;
END;
$function$;
CREATE TRIGGER goodscheck_updatetime_trriger
BEFORE UPDATE OF updatetime ON goodscheck
FOR EACH ROW
EXECUTE PROCEDURE update_timestamp();
