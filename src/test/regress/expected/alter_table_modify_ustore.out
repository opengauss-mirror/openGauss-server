CREATE OR REPLACE FUNCTION tg_bf_test_at_modify_func() RETURNS TRIGGER AS
$$
    DECLARE
    BEGIN
        UPDATE test_at_modify_depend SET b = NULL WHERE a < NEW.a;
        RETURN NEW;
    END
$$ LANGUAGE PLPGSQL;
