\! @abs_bindir@/gs_encrypt -V > @abs_bindir@/test_gs_encrypt.log 2>&1
\! @abs_bindir@/gs_encrypt plaintext > @abs_bindir@/test_gs_encrypt.log 2>&1 ; echo $?
0
\! rm -f @abs_bindir@/tde.key.cipher
\! rm -f @abs_bindir@/tde.key.rand
