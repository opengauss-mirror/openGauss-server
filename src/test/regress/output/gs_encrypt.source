1
\! @abs_bindir@/gs_encrypt -k key -v 0123456789abc plaintext > @abs_bindir@/test_gs_encrypt.log 2>&1 ; echo $?
1
\! @abs_bindir@/gs_encrypt -k ABCdef@123 -v 0123456789abcdef plaintext > @abs_bindir@/test_gs_encrypt.log 2>&1 ; echo $?
0
